    "data_max_value = numpy.amax(input_ltpm_matrix)\n",
    "\n",
    "def noise_prior(batch_size, dim):\n",
    "    temp_norm = np.random.normal(0.0, data_max_value/10, size=(batch_size, dim))\n",
    "    temp_poisson = np.random.poisson(1, size=(batch_size, dim))\n",
    "    return np.abs(temp_norm + temp_poisson)\n",
    "      \n",
    "example_noise_batch = noise_prior(100, 100)\n",
    "example_noise_hist = np.hstack(numpy.ndarray.flatten(example_noise_batch))\n",
    "\n",
    "plt.style.use('seaborn-white')\n",
    "count, bins, ignored = plt.hist(example_noise_hist, bins=100, normed=True, edgecolor=\"none\")\n",
    "plt.title(\"Latent vector distribution\", fontsize=25)\n",
    "plt.xlabel(\"Latent vector magnitude\", fontsize=25)\n",
    "plt.ylabel(\"Density\", fontsize=25)\n",
    "savefig(\"../plots/latent_vector_distribution.pdf\")\n",
    "plt.show()\n"
   ]
